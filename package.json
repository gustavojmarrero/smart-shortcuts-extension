{
  "name": "smart-shortcuts",
  "version": "2.1.1",
  "description": "Accesos directos inteligentes más rápidos que favoritos",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/post-build.js",
    "build:zip": "npm run build && cd dist && zip -r ../smart-shortcuts.zip .",
    "package": "npm run build && node scripts/package-release.cjs",
    "release": "npm run package && node scripts/create-github-release.cjs",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx",
    "icons": "open scripts/create-icon-html.html",
    "prepare": "husky"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.277",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "terser": "^5.44.1",
    "typescript": "^5.6.3",
    "vite": "^6.0.1"
  }
}
